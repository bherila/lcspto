@model IEnumerable<lcspto_mvc.form>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            FormName
        </th>
        <th>
            EmailResponsesTo
        </th>
        <th>
            SaveResponses
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FormName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmailResponsesTo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SaveResponses)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
